{
	"name": "gearbox-studio-playable",
	"version": "5.2.8",
	"description": "playable template for gearbox studio",
	"main": "src/index.js",
	"scripts": {
		"build:normal": "vite build --config .core/config/vite/vite.config.prod.js",
		"build:generate-brotli": "webpack --mode production --config .core/config/webpack.brotli.js",
		"build": "npm run build:normal -brotli && npm run build:generate-brotli",
		"build-analyze": "npm run build:normal -analyze -brotli && npm run build:generate-brotli",
		"start": "vite --mode development --port 0 --open --config .core/config/vite/vite.config.dev.js",
		"start-game": "vite --mode development --config .core/config/vite/vite.config.dev.js",
		"start-gearbox": "vite --mode development --config .core/config/vite/vite.config.dev.js -- --platform=gearbox",
		"dev": "vite --config .core/config/vite/vite.config.dev.js --port 0 --open",
		"stuffs": "webpack-dev-server --port 0 --config .core/config/webpack.stuffs.config.js",
		"stuffs-build": "webpack --mode production --config .core/config/webpack.stuffs.config.js",
		"build-stuffs": "webpack --mode production --config .core/config/webpack.stuffs.config.js",
		"brotlayan": "node .core/config/tools/brotlayan.js"
	},
	"author": "MAPACARTA",
