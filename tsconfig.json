{
	"compilerOptions": {
		// "target": "es5",
		// "lib": ["ES2015", "dom"],
		"target": "es2022",
		"lib": ["dom", "es2022"],
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowJs": true,
		"noEmit": true,
		"typeRoots": ["./node_modules/@types", "./src/@types"],
		"baseUrl": ".",
		"paths": {
			"*": ["/*"],
			"@assets/*": ["./assets/*"],
			"assets/*": ["./assets/*"],
			"@globals": ["./src/globals.ts"],
			"@data": ["./src/params.ts"],
			"@src/*": ["./src/*"],
			"core/init": ["./.core/core/init.ts"],
			"utils2D": ["./.core/utils/utils2D-exports.ts"],
			"utils3D": ["./.core/utils/utils3D-exports.ts"],
			"utils": ["./.core/utils/utils-exports.ts"],
			"core/libs/common/general/networks": ["./.core/core/libs/common/general/networks.ts"],
			"core/libs/pixi/objects/components/components": ["./.core/core/libs/pixi/objects/components/components.ts"],
			"core/libs/three/objects/components/components": ["./.core/core/libs/three/objects/components/components.ts"],
			"core/libs/common/EventEmitter": ["./.core/core/libs/common/EventEmitter.ts"],
			"core/libs/common/script/Script2D": ["./.core/core/libs/common/script/Script2D.ts"],
			"core/libs/common/script/Script3D": ["./.core/core/libs/common/script/Script3D.ts"],
		}
	},
	"include": [
		"src/**/*.ts",
		"assets/scripts/**/*.ts",
		"assets/scripts/**/*.js",
		".core/utils/utils2D-exports.ts",
		".core/utils/utils3D-exports.ts",
		".core/utils/utils-exports.ts",
		".core/core/libs/pixi/objects/prototypes/PixiPrototypes.ts",
		".core/core/libs/pixi/objects/components/components.ts",
		".core/core/libs/three/objects/components/components.ts",
		".core/core/libs/common/EventEmitter.ts",
		".core/core/libs/common/script/Script2D.ts",
		".core/core/libs/common/script/Script3D.ts",

	],
	"exclude": [
		".core/core/*",
		".core/utils/utils2D-impl.ts",
		".core/utils/utils3D-impl.ts",
		".core/utils/utils-impl.ts"
	]
}
